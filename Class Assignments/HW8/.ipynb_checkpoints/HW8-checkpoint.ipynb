    "import numpy as np\n",
    "from numpy import linalg as LA\n",
    "\n",
    "x = np.array([[0, 0], [3, 0], [1, 2], [4, 2]])\n",
    "xm = np.mean(x, axis=0)\n",
    "xc = np.cov(x, rowvar=False)\n",
    "eig_vals, eig_vecs = LA.eigh(xc)\n",
    "print(xm)\n",
    "print(xc)\n",
    "print(eig_vals)\n",
    "print(eig_vecs)"
   ]
